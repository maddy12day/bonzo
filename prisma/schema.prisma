generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model daily_inv_by_location_by_sku {
  id             BigInt    @id @default(autoincrement())
  channel        String?   @db.VarChar(10)
  country        String?   @db.VarChar(5)
  inv_date       DateTime? @db.Date
  sku            String?   @db.VarChar(50)
  start_balance  BigInt?
  total_inputs   BigInt?
  total_outputs  BigInt?
  end_balance    BigInt?
  ods_created_at DateTime? @default(now()) @db.Timestamp(0)
  ods_updated_at DateTime? @default(now()) @db.Timestamp(0)

  @@index([channel], name: "daily_inv_by_location_by_sku_channel_IDX")
  @@index([country], name: "daily_inv_by_location_by_sku_country_IDX")
  @@index([inv_date], name: "daily_inv_by_location_by_sku_inv_date_IDX")
  @@index([sku], name: "daily_inv_by_location_by_sku_sku_IDX")
}

