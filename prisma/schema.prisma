model scenario_types {
  id                 Int       @id @default(autoincrement())
  scenario_type      String?   @db.VarChar(30)
  scenario_type_desc String?   @db.VarChar(100)
  created_at         DateTime? @default(now()) @db.DateTime(0)
  scenarios          scenarios[]
  @@index([scenario_type], name: "scenario_types_scenario_type_IDX")
}

model scenarios {
  id                     Int       @id @default(autoincrement())
  demand_planner_user_id Int
  scenario_name          String    @db.VarChar(50)
  amount                 Float?    @db.Float
  is_dollar              Boolean?
  start_date             DateTime  @db.DateTime(0)
  end_date               DateTime? @db.DateTime(0)
  filter_level           String?   @db.VarChar(20)
  filter_product_sources String?   @db.VarChar(100)
  filter_brand_types     String?   @db.VarChar(100)
  filter_life_cycles     String?   @db.VarChar(255)
  filter_newness         String?   @db.VarChar(30)
  filter_brands          String?   @db.VarChar(255)
  filter_channels        String?   @db.VarChar(10)
  filter_sub_channels    String?   @db.VarChar(1000)
  filter_collections     String?   @db.VarChar(1000)
  filter_skus            String?   @db.VarChar(1000)
  filter_programs        String?   @db.VarChar(255)
  filter_categories      String?   @db.VarChar(255)
  filter_classes         String?   @db.VarChar(500)
  filter_sub_classes     String?   @db.VarChar(1000)
  is_active              Boolean?
  status                 String?   @db.VarChar(20)
  is_shared              Boolean?
  is_part_of_base        Boolean?
  merged_with_base_at    DateTime? @db.DateTime(0)
  merged_with_base_id    Int?
  created_at             DateTime? @default(now()) @db.DateTime(0)
  scenario_types         scenario_types  @relation(fields: [scenario_type_id], references: [id])
  scenario_type_id       Int
  @@index([demand_planner_user_id], name: "scenarios_demand_planner_user_id_IDX")
}

model users {
  id           Int       @id @default(autoincrement())
  email_id     String?   @db.VarChar(255)
  password     String?   @db.VarChar(255)
  first_name   String?   @db.VarChar(50)
  last_name    String?   @db.VarChar(50)
  account_id   Int
  user_role_id Int
  created_at   DateTime? @default(now()) @db.Timestamp(0)
  updated_at   DateTime? @default(now()) @db.Timestamp(0)
}
